@model BolsaTrabajoV1.Models.EMPRESA

<div class="col-md-offset-4 col-md-12">
    <h4>
        EMPRESA: @Html.DisplayFor(model => model.NOMBREEMPRESA)
        @Html.ActionLink("Editar", "Edit", new { id = Model.CODIGOEMPRESA }, new { @class = "btn btn-primary" })
    </h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           CODIGO:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CODIGOEMPRESA)
        </dd>
        <dt>
           NOMBRE:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NOMBREEMPRESA)
        </dd>
        <br>
        <dt>
           ABREVIATURA:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ABREVIATURA)
        </dd>
        <br>

        <dt>
          CORREO ELECTRONICO:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CORREOELECTRONICO)
        </dd>
        <br>
        <dt>
           TELEFONO:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TELEFONO)
        </dd>
        <br>
        <dt>
            PAIS
        </dt>
        <dd>
            @TempData["nPais"]
        </dd>
        <br>
        <dt>
           DEPARTAMENTO
        </dt>
        <dd>
            @TempData["nDept"]
        </dd>
        <br>
        <dt>
            MUNICIPIO
        </dt>
        <dd>
            @TempData["nMn"]
        </dd>
        <br>
        <dt>
            NIT:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NIT)
        </dd>
        <br>
        <dt>
            GIRO:
        </dt>

        <dd>
            @TempData["nGiro"]
        </dd>
        <br>
        <dt>
            DESCRIPCION:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DESCRIPCION)
        </dd>
    </dl>
</div>

