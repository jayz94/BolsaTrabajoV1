@model IEnumerable<BolsaTrabajoV1.Models.CERTIFICACION>
    <h1 class="text-center">Certificaciones</h1>
<p>
    @Html.ActionLink("Agregar", "Create",null, new { @class="btn btn-success"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TITULOCERTIFICACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHAINICIOCERTIFICACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHAFINCERTIFICACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CODIGOCERTIFICACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.INSTITUCION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AREA_CONOCIMIENTO.NOMBREAREACONOCIMIENTO)
        </th>
        <th>Acciones</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TITULOCERTIFICACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FECHAINICIOCERTIFICACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FECHAFINCERTIFICACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CODIGOCERTIFICACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.INSTITUCION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AREA_CONOCIMIENTO.NOMBREAREACONOCIMIENTO)
        </td>
        <td>
            @Html.ActionLink(" ", "Edit", new { id = item.IDCERTIFICACION }, new { @class = "btn btn-success fa fa-edit btn-xs" })
            @Html.ActionLink(" ", "Details", new { id = item.IDCERTIFICACION }, new { @class = "btn btn-primary fa fa-search btn-xs" } )
            @Html.ActionLink(" ", "Delete", new { id = item.IDCERTIFICACION }, new { @class = "btn btn-danger fa fa-trash btn-xs" })
        </td>
    </tr>
}
</table>
<a class="btn btn-danger" style="margin-top:5%" href='@Url.Action("MainCurriculum", "Curriculum")'>Regresar
</a>