@model IEnumerable<BolsaTrabajoV1.Models.PARTICIPACION_PROFESIONAL>

<h2 class="text-center">Participaciones Profesionales</h2>

<p>
    @Html.ActionLink("Agregar", "Create", null, new { @class = "btn btn-success" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TITULOPARTICIPACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LUGARPARTICIPACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHAPARTICIPACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ANFITRION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PAIS.NOMBREPAIS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TIPO_PARTICIPACION.NOMBRETIPOPARTICIPACION)
        </th>
        <th>Acciones</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TITULOPARTICIPACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LUGARPARTICIPACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FECHAPARTICIPACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ANFITRION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PAIS.NOMBREPAIS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TIPO_PARTICIPACION.NOMBRETIPOPARTICIPACION)
        </td>
        <td>
            @Html.ActionLink(" ", "Edit", new { id = item.IDPARTICIPACION }, new { @class = "btn btn-success fa fa-edit btn-xs" })
            @Html.ActionLink(" ", "Details", new { id = item.IDPARTICIPACION }, new { @class = "btn btn-primary fa fa-search btn-xs" } )
            @Html.ActionLink(" ", "Delete", new { id = item.IDPARTICIPACION }, new { @class = "btn btn-danger fa fa-trash btn-xs" })
        </td>
    </tr>
}

</table>
<a class="btn btn-danger" style="margin-top:5%" href='@Url.Action("MainCurriculum", "Curriculum")'>
    Regresar
</a>