@model IEnumerable<BolsaTrabajoV1.Models.PUBLICACION>

<h2 class="text-center">Publicaciones</h2>

<p>
    @Html.ActionLink("Agregar", "Create", null, new { @class = "btn btn-success" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TITULOPUBLICACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHAPUBLICACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ISBN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LIBRO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NUMEROEDICION)
        </th>
        <th>Acciones</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TITULOPUBLICACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FECHAPUBLICACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ISBN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LIBRO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NUMEROEDICION)
        </td>
        <td>
            @Html.ActionLink(" ", "Edit", new { id = item.IDPUBLICACION }, new { @class = "btn btn-success fa fa-edit btn-xs" })
            @Html.ActionLink(" ", "Details", new { id = item.IDPUBLICACION }, new { @class = "btn btn-primary fa fa-search btn-xs" } ) 
            @Html.ActionLink(" ", "Delete", new { id = item.IDPUBLICACION }, new { @class = "btn btn-danger fa fa-trash btn-xs" })
        </td>
    </tr>
}

</table>
<a class="btn btn-danger" style="margin-top:5%" href='@Url.Action("MainCurriculum", "Curriculum")'>
    Regresar
</a>